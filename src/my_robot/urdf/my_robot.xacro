<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />
  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".25 .2 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".25 .2 .1"/>
      </geometry>
    </visual>

    <collision name='top_collision'>
      <origin xyz="0 0 .075" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".15 .19 .075"/>
      </geometry>
    </collision>

    <visual name='top_visual'>
      <origin xyz="0 0 0.075" rpy=" 0 0 0"/>
      <geometry>
        <box size=".15 .19 .075"/>
      </geometry>
    </visual>

  </link>

  <link name='right_f_wheel'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='right_f_wheel_collision'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0425" length="0.035"/>
      </geometry>
    </collision>

    <visual name='right_f_wheel_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0425" length="0.035"/>
      </geometry>
    </visual>

  </link>

  <link name='left_f_wheel'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='left_f_wheel_collision'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0425" length="0.035"/>
      </geometry>
    </collision>

    <visual name='left_f_wheel_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0425" length="0.035"/>
      </geometry>
    </visual>

  </link>

  <link name='right_r_wheel'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='right_r_wheel_collision'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0425" length="0.035"/>
      </geometry>
    </collision>

    <visual name='right_r_wheel_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0425" length="0.035"/>
      </geometry>
    </visual>

  </link>

  <link name='left_r_wheel'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='left_r_wheel_collision'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0425" length="0.035"/>
      </geometry>
    </collision>

    <visual name='left_r_wheel_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.0425" length="0.035"/>
      </geometry>
    </visual>

  </link>

  <joint type="continuous" name="right_f_wheel_hinge">
    <origin xyz="0.1 -0.125 -0.025" rpy="0 0 0"/>
    <child link="right_f_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="left_f_wheel_hinge">
    <origin xyz="0.1 0.125 -0.025" rpy="0 0 0"/>
    <child link="left_f_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="right_r_wheel_hinge">
    <origin xyz="-0.1 -0.125 -0.025" rpy="0 0 0"/>
    <child link="right_r_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="left_r_wheel_hinge">
    <origin xyz="-0.1 0.125 -0.025" rpy="0 0 0"/>
    <child link="left_r_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <link name='camera'>
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <box_inertia m="0.1" x="0.05" y="0.05" z="0.025}"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <collision name='camera_collision'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".025 .05 .05"/>
      </geometry>
    </collision>

    <visual name='camera_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".025 .05 .05"/>
      </geometry>
    </visual>

  </link>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.075 0 0.085 " rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <link name='camera_link_optical'>
  </link>

  <joint name="camera_optical_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="camera"/>    
    <child link="camera_link_optical"/>
  </joint>

  <link name='hokuyo'>
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="1e-5"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <box_inertia m="0.1" x="0.05" y="0.05" z="0.05}"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <collision name='hokuyo_collision'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".1 .1 .1"/>
      </geometry>
    </collision>

    <visual name='hokuyo_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <mesh filename='package://my_robot/meshes/hokuyo.dae'/>
      </geometry>
    </visual>

  </link>

  <joint type="fixed" name="hokuyo_joint">
    <origin xyz="0 0 0.15" rpy="0 0 0"/>
    <child link="hokuyo"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <gazebo reference="chassis">
    <material>Gazebo/Blue</material>
  </gazebo>
  <gazebo reference="camera">
    <material>Gazebo/Black</material>
  </gazebo>
  <gazebo reference="right_f_wheel">
    <material>Gazebo/FlatBlack</material>
  </gazebo>
  <gazebo reference="left_f_wheel">
    <material>Gazebo/FlatBlack</material>
  </gazebo>
  <gazebo reference="right_r_wheel">
    <material>Gazebo/FlatBlack</material>
  </gazebo>
  <gazebo reference="left_r_wheel">
    <material>Gazebo/FlatBlack</material>
  </gazebo>

</robot>
